#spdif
    insmod /system/vendor/modules/sun6i_spdif.ko
    insmod /system/vendor/modules/sun6i_spdma.ko
    insmod /system/vendor/modules/sndspdif.ko
    insmod /system/vendor/modules/sun6i_sndspdif.ko

service pvrsrvctl /system/vendor/bin/pvrsrvctl --start --no-module
    class core
    user root
    group root
    oneshot
# updatesoftwinner service
service updatesoftwinner /system/bin/updatesoftwinner
    class main
    user  root
    group root

# ril related services
service ril-daemon /system/bin/rild -l libsoftwinner-ril.so -- -d /dev/ttyUSB2
    class main
    socket rild stream 660 root radio
    socket rild-debug stream 660 radio system
    user root
    group radio cache inet misc audio sdcard_rw log

#service logcat /system/bin/logcat -r1000 -n 10 -v time -f /data/local/logcat.log
#    class main
#logger service: logger [kernel|android|radio] 
#service logger_kernel /system/bin/logger.sh kernel
